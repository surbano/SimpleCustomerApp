environment{
        CHECK_URL = "https://stackoverflow.com"
        CMD = "curl --write-out %{http_code} --silent --output /dev/null ${CHECK_URL}"

    }

def someMethod(varResultado) {
    
    
	echo varResultado
	
	sh "${CMD} > commandResult"
                    env.status = readFile('commandResult').trim()
		    
		    
	sh "echo ${env.status}"
                    if (env.status == '200') {
                        currentBuild.result = "SUCCESS"
                    }
                    else {
                        currentBuild.result = "FAILURE"
                    }    
		
	
	if(varResultado == 'OK'){
		//build job:  '/prueba-demo1', parameters: [string(name: 'param1', value:'val1')], wait: true  
	}	
}

return this
